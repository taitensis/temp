{
  "name": "astro-app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"npm run dev:astro\" \"npm run lint:watch\" \"npm run prettier:watch\"",
    "dev:astro": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.astro",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx,.astro --fix",
    "lint:watch": "nodemon --watch '**/*.{ts,tsx,js,jsx,astro}' --exec 'npm run lint:fix'",
    "prettier:watch": "onchange '**/*.{js,ts,jsx,tsx,astro,json,md}' -- prettier --write {{changed}}",
    "format": "npm run lint:fix && prettier --write .",
    "check:format": "prettier --check ."
  },
  "dependencies": {
    "@astrojs/react": "^4.4.0",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.76.1",
    "@tailwindcss/vite": "^4.1.14",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "astro": "^5.15.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-astro": "^0.548.0",
    "lucide-react": "^0.548.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "astro-eslint-parser": "^1.2.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.4.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "onchange": "^7.1.0",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.4.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
